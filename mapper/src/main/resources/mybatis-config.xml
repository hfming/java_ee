<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!--mybatis 主配置文件-->
<configuration>
    <settings>
<!--        开启二缓存-->
        <setting name="cacheEnabled" value="true"/>
    </settings>

<!--    全局类型转换器-->
    <typeHandlers>
        <typeHandler
                handler="com.hfm.typehandler.AddressTypeHandler"
                javaType="com.hfm.pojo.Address"/>
    </typeHandlers>
</configuration>